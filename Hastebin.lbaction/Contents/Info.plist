<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

    <key>CFBundleIdentifier</key>
    <string>io.henrik.launchbar.Hastebin</string>

    <key>CFBundleName</key>
    <string>Hastebin</string>

    <!-- <key>CFBundleShortVersionString</key> -->
    <!-- <value>1.0</value> -->

    <key>CFBundleVersion</key>
    <string>1.2.1</string>

    <key>CFBundleIconFile</key>
    <string>at.obdev.LaunchBar:ContactNote</string>

    <key>LBDebugLogEnabled</key>
    <false />

    <!-- LB stuff -->
    <!-- <key>LSMinimumSystemVersion</key> -->
    <!-- <string>10.9.1</string> -->

    <!-- Per-action keys -->
    <key>LBTextInputTitle</key>
    <string>Send to Hastebin</string>

    <key>LBScripts</key>
    <dict>
        <key>LBDefaultScript</key>
        <dict>
            <key>LBScriptName</key>
            <string>default.js</string>

            <key>LBRunInBackground</key>
            <false />

            <key>LBRequiresArgument</key>
            <false />

            <key>LBAcceptedArgumentTypes</key>
            <array>
                <string>string</string>
                <string>path</string>
            </array>

            <key>LBReturnsResult</key>
            <true/>

            <!-- <key>LBReturnType</key> -->
        </dict>
    </dict>

    <key>LBDescription</key>
    <dict>
        <key>LBAuthor</key>
        <string>Henrik Lissner</string>

        <key>LBWebsite</key>
        <string>http://v0.io/launchbar</string>

        <key>LBEmail</key>
        <string>henrik@lissner.net</string>

        <key>LBTwitter</key>
        <string>@vnought</string>

        <key>LBSummary</key>
        <string>Sends text (or text files) to hastebin and returns their url</string>

        <key>LBArgument</key>
        <string>The text to be sent to hastebin</string>

        <key>LBResult</key>
        <string>An URL</string>

        <key>LBRequirements</key>
        <string>An internet connection</string>

        <key>LBChangelog</key>
        <string>Added auto-update support</string>

        <key>LBUpdateURL</key>
        <string>https://raw.githubusercontent.com/hlissner/lb6-actions/master/Hastebin.lbaction/Contents/Info.plist</string>

        <key>LBDownloadURL</key>
        <string>http://dl.v0.io/launchbar/Hastebin.lbaction</string>


    </dict>

</dict>
</plist>
